# Anthropic (required for planner and code generator)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Zapier MCP (required for creating agents that use Trello, Slack, etc.)
# Get URL and secret from https://mcp.zapier.com
ZAPIER_MCP_SERVER_URL=
ZAPIER_MCP_SECRET=

# LangSmith tracing (see docs/TESTING.md)
LANGSMITH_API_KEY=your-langsmith-api-key
LANGCHAIN_TRACING_V2=true
# Optional: send traces to a dedicated project
# LANGCHAIN_PROJECT=FuseAI-planner
